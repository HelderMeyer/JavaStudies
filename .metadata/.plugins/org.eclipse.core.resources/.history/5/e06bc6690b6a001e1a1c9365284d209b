package roteiro1.parte2;

public class Exercicio01 {
    public static void main(String [] args){
        double sb = 2500;
        int numHorasExtra = 10;
        double horasPaga =  sb/Double.valueOf(40);
        double salarioFinal = sb+(Double.valueOf(numHorasExtra)*horasPaga);
        
        System.out.println("Salario base: "+salarioFinal);
    }
}
